{
    "name": "owl/suggestion-bundle",
    "type": "symfony-bundle",
    "description": "Suggestions management for Symfony applications.",
    "keywords": [
        "suggestion",
        "owl"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Kęska",
            "email": "projekty@pawelkeska.eu"
        }
    ],
    "require": {
        "php": "^8.0",
        "stof/doctrine-extensions-bundle": "^1.4",
        "owl/suggestion": "^1.0",
        "sylius/resource-bundle": "^1.7",
        "symfony/framework-bundle": "^4.4 || ^5.2"
    },
    "require-dev": {
        "doctrine/orm": "^2.8",
        "symfony/dependency-injection": "^5.4",
        "symfony/form": "^5.4",
        "symfony/validator": "^5.4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Owl\\Bundle\\SuggestionBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
        },
        "classmap": [
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
